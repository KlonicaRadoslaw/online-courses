@model int

<a asp-action="Edit" asp-route-id="@Model" class="text-info m-2"><i class="fas fa-edit"></i></a> 
<a asp-action="Details" asp-route-id="@Model" class="text-dark m-2"><i class="fas fa-table"></i></a> 
<a asp-action="Delete" asp-route-id="@Model" class="text-danger m-2"><i class="fas fa-trash"></i></a> 